@model Lab3.Models.Person
@{
    Layout = "_Layout";
    ViewData["Title"] = "DisplayPerson";
}
<h3>Your Favorite Person</h3>

<table>
    <tr>
        <td>
            First Name: @Model.FirstName
        </td>
    </tr>
    <tr>
        <td>
            Last Name: @Model.LastName
        </td>
    </tr>
    <tr>
        <td>
            Age: @Model.Age
        </td>
    </tr>
    <tr>
        <td>
            Email Address: @Model.Email
        </td>
    </tr>
    <tr>
        <td>
            Date of Birth: @Model.Dob
        </td>
    </tr>
    <tr>
        <td>
            Description: @ViewBag.Description
        </td>
    </tr>
</table>
